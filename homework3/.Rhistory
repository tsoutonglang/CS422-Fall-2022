library(rpart)
library(rpart.plot)
data(iris)
head(iris)
model <- rpart(Species ~ ., data=iris, method="class")
summary(model)
rpart.plot(model, extra=104, fallen.leaves=T, type=4, main="Iris Dataset Decision Tree")
print("There are three levels.")
cat(print("Level 1, Vertex 1: Default class label is setosa\nLevel 2, Vertex 1: Default class label is setosa\nLevel 2, Vertex 2: Default class label is versicolor\nLevel 3, Vertex 1: Default class label is versicolor\nLevel 3, Vertex 1: Default class label is virginica\n"))
cat(print("Level 1, Vertex 1: Default class label is setosa\nLevel 2, Vertex 1: Default class label is setosa\nLevel 2, Vertex 2: Default class label is versicolor\nLevel 3, Vertex 1: Default class label is versicolor\nLevel 3, Vertex 1: Default class label is virginica\n"))
cat(print("Level 1, split on attribute: Petal.Length\nSplit points: < 2.45 left subtree, ≥ 2.45 right subtree\n
Level 2, split on attribute: Petal. Width\nSplit Points: < 0.8 left subtree, ≥ 0.8 right subtree"))
